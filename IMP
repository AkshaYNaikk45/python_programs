package Games;

public class Indoor {
    String gameName;
    String[] players;

    // Default constructor
    public Indoor() {
        gameName = "Chess";
        players = new String[]{"Amit", "Sneha", "Rahul"};
    }

    // Parameterized constructor
    public Indoor(String gameName, String[] players) {
        this.gameName = gameName;
        this.players = players;
    }

    public void display() {
        System.out.println("Indoor Game: " + gameName);
        System.out.println("Players:");
        for (String p : players) {
            System.out.println("- " + p);
        }
    }
}


package Games;

public class Outdoor {
    String gameName;
    String[] players;

    // Default constructor
    public Outdoor() {
        gameName = "Cricket";
        players = new String[]{"Rohit", "Virat", "Dhoni"};
    }

    // Parameterized constructor
    public Outdoor(String gameName, String[] players) {
        this.gameName = gameName;
        this.players = players;
    }

    public void display() {
        System.out.println("Outdoor Game: " + gameName);
        System.out.println("Players:");
        for (String p : players) {
            System.out.println("- " + p);
        }
    }
}



import Games.*;   // Import package

public class MainGame {
    public static void main(String[] args) {
        // Using default constructors
        Indoor i1 = new Indoor();
        Outdoor o1 = new Outdoor();

        // Using parameterized constructors
        String[] badmintonPlayers = {"Neeraj", "Prakash", "Kiran"};
        Indoor i2 = new Indoor("Badminton", badmintonPlayers);

        String[] footballPlayers = {"Messi", "Ronaldo", "Neymar"};
        Outdoor o2 = new Outdoor("Football", footballPlayers);

        // Display details
        i1.display();
        System.out.println();
        o1.display();
        System.out.println();
        i2.display();
        System.out.println();
        o2.display();
    }
}



18) multilevelinheritacne


class Employee {
    String name;
    double salary;

   
    Employee(String name, double salary) {
        this.name = name;
        this.salary = salary;
    }

 
    void displayEmployeeDetails() {
        System.out.println("Employee Name: " + name);
        System.out.println("Salary: " + salary);
    }
}

class Developer extends Employee {
    String projectName;

    
    Developer(String name, double salary, String projectName) {
        super(name, salary); // Call to Employee constructor
        this.projectName = projectName;
    }

    
    void displayDeveloperDetails() {
        displayEmployeeDetails(); // Call method from Employee
        System.out.println("Project Name: " + projectName);
    }
}

class Programmer extends Developer {
    String progLanguage;

    
    Programmer(String name, double salary, String projectName, String progLanguage) {
        super(name, salary, projectName); // Call to Developer constructor
        this.progLanguage = progLanguage;
    }

  
    void displayProgrammerDetails() {
        displayDeveloperDetails(); 
        System.out.println("Programming Language: " + progLanguage);
    }
}

public class MultilevelInheritanceDemo {
    public static void main(String[] args) {
        
        Programmer pr = new Programmer("Alice", 75000, "Banking App"," Java");

        System.out.println("Programmer Details:");
        pr.displayProgrammerDetails();
    }
}


19) multiplication

import java.io.*;

public class MultiplicationTable {
    public static void main(String[] args) throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

        System.out.print("Enter the number up to which table should be printed: ");
        int num = Integer.parseInt(br.readLine());

        System.out.println("***** MULTIPLICATION TABLE *****");

        for (int i = 1; i <= num; i++) {
            for (int j = 1; j <= 10; j++) {
                System.out.print(" " + (i * j) + " ");
            }
            System.out.println(); 
        }
    }
}


20) mydate

import java.util.Scanner;

public class MyDate {
    int dd, mm, yy;

    // Default constructor
    MyDate() {
        this.dd = 1;
        this.mm = 1;
        this.yy = 2000;
    }

    // Parameterized constructor
    MyDate(int dd, int mm, int yy) {
        this.dd = dd;
        this.mm = mm;
        this.yy = yy;
    }

    // Accept input from user
    void accept() {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter day: ");
        this.dd = sc.nextInt();
        System.out.print("Enter month: ");
        this.mm = sc.nextInt();
        System.out.print("Enter year: ");
        this.yy = sc.nextInt();
    }

    // Method to display date
    void display() {
        System.out.println(this.dd + "-" + this.mm + "-" + this.yy);
    }

    // Optional main for testing MyDate separately
    public static void main(String[] args) {
        MyDate date1 = new MyDate();
        System.out.print("Default Date: ");
        date1.display();

        MyDate date2 = new MyDate(4, 8, 2025);
        System.out.print("Parameterized Date: ");
        date2.display();
    }
}


21)overloading

import java.util.Scanner;

public class OverloadingMethod {

    // Area of Circle
    double area(double radius) {
        return 3.14 * radius * radius;
    }

    // Area of Rectangle
    double area(double length, double breadth) {
        return length * breadth;
    }

    // Area of Triangle
    double area(double base, double height, boolean isTriangle) {
        return 0.5 * base * height;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        OverloadingMethod ac = new OverloadingMethod();

        System.out.print("Enter radius of circle: ");
        double r = sc.nextDouble();
        System.out.println("Area of Circle: " + ac.area(r));

        System.out.print("\nEnter length and breadth of rectangle: ");
        double l = sc.nextDouble();
        double b = sc.nextDouble();
        System.out.println("Area of Rectangle: " + ac.area(l, b));

        System.out.print("\nEnter base and height of triangle: ");
        double base = sc.nextDouble();
        double h = sc.nextDouble();
        System.out.println("Area of Triangle: " + ac.area(base, h, true));

        sc.close();
    }
}

22) primecheck

import java.util.Scanner;

// User defined exception
class ZeroNumberException extends Exception {
    public ZeroNumberException(String message) {
        super(message);
    }
}

public class PrimeCheck {
    
    // Function to check prime
    static boolean isPrime(int n) {
        if (n <= 1) return false;
        for (int i = 2; i <= n / 2; i++) {
            if (n % i == 0)
                return false;
        }
        return true;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        try {
            System.out.print("Enter a number: ");
            int num = sc.nextInt();

            if (num == 0) {
                throw new ZeroNumberException("Number is 0, not allowed!");
            }

            if (isPrime(num)) {
                System.out.println(num + " is Prime");
            } else {
                System.out.println(num + " is Not Prime");
            }
        } catch (ZeroNumberException e) {
            System.out.println("Exception: " + e.getMessage());
        }
        sc.close();
    }
}



23) product

import java.io.*;

class ProductInfo {
    int pid;
    String n;
    int m;

    ProductInfo(int pid, String n, int m) {
        this.pid = pid;
        this.n = n;
        this.m = m;
    }

    public void display() {
        System.out.print("Product Id = " + pid + "  ");
        System.out.print("Product Name = " + n + "  ");
        System.out.println("Product Price = " + m);
    }
}

public class Product {

    public static void main(String args[]) {
        try {
            BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

            ProductInfo[] obj = new ProductInfo[5];

            int pro_Id;
            String pro_name;
            int pro_pr;

            for (int i = 0; i < 5; i++) {
                System.out.println("Enter Product ID");
                pro_Id = Integer.parseInt(br.readLine());

                System.out.println("Enter Product Name");
                pro_name = br.readLine();

                System.out.println("Enter Product Price");
                pro_pr = Integer.parseInt(br.readLine());

                obj[i] = new ProductInfo(pro_Id, pro_name, pro_pr);
            }

            for (int i = 0; i < 5; i++) {
                obj[i].display();
            }

        } catch (Exception e) {
            System.out.println("Something went wrong.");
        }
    }
}


24) reverse

public class ReverseNumber {
    public static void main(String[] args) {
        int num = Integer.parseInt(args[0]);
        int reversed = 0;
        while (num != 0) {
            int digit = num % 10;
            reversed = reversed * 10 + digit;
            num /= 10;
        }
        System.out.println("Reversed Number: " + reversed);
    }
}

