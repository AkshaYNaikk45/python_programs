Accountsort.java

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

class Account {
    int acno;            
    String acname;       
    float bal;          
    static int count;   

    // Default constructor
    Account() {}

    // Parameterized constructor
    Account(String n, float p) {
        count++;          
        acno = count;     
        acname = n;
        bal = p;
    }

    // Method to display account details
    void display() {
        System.out.println(acno + "\t" + acname + "\t" + bal);
    }

    // Method to return balance
    float getbal() {
        return bal;
    }

    // Static method to show how many objects are created
    static void counter() {
        System.out.println(count + " object is created");
    }

    // Static method to sort accounts by balance in ascending order using Bubble Sort
    public static void sortAccount(Account a[], int n) {
        // Outer loop for number of passes (starts from last index and moves to 0)
        for (int i = n - 1; i >= 0; i--) {
            // Inner loop for comparing adjacent elements from start to i
            for (int j = 0; j < i; j++) {
                // If current account's balance is greater than next, swap them
                if (a[j].getbal() > a[j + 1].getbal()) {                    
                    Account t = a[j];
                    a[j] = a[j + 1];
                    a[j + 1] = t;
                }
            }
        }

        // Display sorted accounts
        System.out.println("\nAccounts sorted by balance:");
        for (int i = 0; i < n; i++) {
            a[i].display();
        }
    }
}

// Main class to test the Account functionality
public class AccountSort {
    public static void main(String args[]) throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));        
        
        System.out.println("Enter number of Accounts:");
        int n = Integer.parseInt(br.readLine());

        Account p[] = new Account[n];

        // Accept account details from user
        for (int i = 0; i < n; i++) {
            System.out.print("Enter Name: ");
            String name = br.readLine();
            System.out.print("Enter Balance: ");
            float bal = Float.parseFloat(br.readLine());

            // Create account object and store it in array
            p[i] = new Account(name, bal);
            p[i].counter();  // Display count after each creation
        }

        // Sort and display accounts based on balance
        Account.sortAccount(p, Account.count);
    }
}




2) Area calculation.java

import java.io.*;
abstract class Shape{
 abstract public void calc_area();
 abstract public void calc_volume();
 final float pi=3.14f;
}

class Sphere extends Shape{
 double r;
 private double area;
 private double volume;
 public void accept() throws IOException{
  System.out.println("Enter the radius of the Sphere: ");
  BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
  r=Double.parseDouble(br.readLine());
 }
 public void calc_area(){
  area=4*pi*r*r;
 }
 public void calc_volume(){
  
  volume=1.33333333334*pi*r*r*r;
 }
 public void display(){
  calc_area();
  calc_volume();
  System.out.println("The area of sphere is: "+area);
  System.out.println("The volume of sphere is: "+volume);
 }
}

class Cone extends Shape{
 double h,r,area,volume;

 public void accept() throws IOException{
  System.out.println("Enter radius and height of the Cone: ");
  BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
  r=Double.parseDouble(br.readLine());
  h=Double.parseDouble(br.readLine());
 }
 public void calc_area(){
  
  double sq=h*h+r*r;
  area=pi*r*(r+ java.lang.Math.sqrt(sq));
 }
 public void calc_volume(){
  double d=h/3;
  volume=pi*r*r*d;
 }
 public void display(){
  calc_area();
  calc_volume();
  System.out.println("The area of Cone is: "+area);
  System.out.println("The volume of Cone is: "+volume);
 }
}

class Cylinder extends Shape{
 double r,h,area,volume;
 public void accept() throws IOException{
  System.out.println("Enter radius and height of the Cylinder: ");
  BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
  r=Double.parseDouble(br.readLine());
  h=Double.parseDouble(br.readLine());
 }
 public void calc_area(){
  area=(2*pi*r*h)+(2*pi*r*r);
 }
 public void calc_volume(){
  volume=pi*r*r*h;
 }
 public void display(){
  calc_area();
  calc_volume();
  System.out.println("The area of Cylinder is: "+area);
  System.out.println("The volume of Cylinder is: "+volume);
 }
}

class Box extends Shape{
 double l,b,h,area,volume;
 public void accept() throws IOException{
  System.out.println("Enter length, breadth and height of the Box: ");
  BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
  l=Double.parseDouble(br.readLine());
  b=Double.parseDouble(br.readLine());
  h=Double.parseDouble(br.readLine());
 }
 public void calc_area(){
  area=(2*l*b)+(2*b*h)+(2*l*h);
 }
 public void calc_volume(){
  volume=l*b*h;
 }
 public void display(){
  calc_area();
  calc_volume();
  System.out.println("The area of Box is: "+area);
  System.out.println("The volume of Box is: "+volume);
 }
}

public class AreaCalculation {
 public static void main(String [] args)throws IOException{
  Sphere s=new Sphere();
  s.accept();
  s.display();
  Cone co=new Cone();
  co.accept();
  co.display();
  Cylinder cy=new Cylinder();
  cy.accept();
  cy.display();
  Box b=new Box();
  b.accept();
  b.display(); 
 }
}


3) ARMSTRONGnumber

import java.util.Scanner;

public class ArmstrongandFactors {
    
    // Function to check Armstrong number
    static boolean isArmstrong(int n) {
        int temp = n, sum = 0, digits = 0;
        while (temp > 0) {
            digits++;
            temp /= 10;
        }
        temp = n;
        while (temp > 0) {
            int d = temp % 10;
            sum += Math.pow(d, digits);
            temp /= 10;
        }
        return sum == n;
    }

    // Function to print factors of a number
    static void printFactors(int n) {
        System.out.print("Factors of " + n + ": ");
        for (int i = 1; i <= n; i++) {
            if (n % i == 0) {
                System.out.print(i + " ");
            }
        }
        System.out.println();
    }

    // Function to print Armstrong numbers up to a given range
    static void printArmstrongRange(int range) {
        System.out.println("Armstrong numbers up to " + range + ":");
        for (int i = 1; i <= range; i++) {
            if (isArmstrong(i)) {
                System.out.print(i + " ");
            }
        }
        System.out.println();
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("Enter a number: ");
        int num = sc.nextInt();

        System.out.print("Enter range: ");
        int range = sc.nextInt();

        if (isArmstrong(num)) {
            System.out.println(num + " is an Armstrong number.");
        } else {
            System.out.println(num + " is NOT an Armstrong number.");
        }

        printFactors(num);
        printArmstrongRange(range);

        sc.close();
    }
}


4)Arraylist

import java.util.ArrayList;
import java.util.Scanner;

public class ArrayListExample {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        ArrayList<Integer> list = new ArrayList<>();

        System.out.print("Enter number of integers: ");
        int n = sc.nextInt();

        System.out.println("Enter " + n + " integers:");
        for (int i = 0; i < n; i++) {
            int num = sc.nextInt();
            list.add(num);
        }

        System.out.println("\nArrayList elements: " + list);
        sc.close();
    }
}



5)CitySTDapp

import java.util.*;

public class CitySTDApp {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        HashMap<String, String> cityMap = new HashMap<>();

        while (true) {
            System.out.println("\n--- City STD Code App ---");
            System.out.println("1. Add City & STD Code");
            System.out.println("2. Remove City");
            System.out.println("3. Search City");
            System.out.println("4. Display All");
            System.out.println("5. Exit");
            System.out.print("Enter your choice: ");
            int choice = sc.nextInt();
            sc.nextLine(); // consume newline

            switch (choice) {
                case 1: // Add
                    System.out.print("Enter City Name: ");
                    String city = sc.nextLine();
                    if (cityMap.containsKey(city)) {
                        System.out.println("City already exists! Duplicate not allowed.");
                    } else {
                        System.out.print("Enter STD Code: ");
                        String code = sc.nextLine();
                        cityMap.put(city, code);
                        System.out.println("City added successfully!");
                    }
                    break;

                case 2: // Remove
                    System.out.print("Enter City to Remove: ");
                    String remCity = sc.nextLine();
                    if (cityMap.remove(remCity) != null) {
                        System.out.println(remCity + " removed successfully!");
                    } else {
                        System.out.println("City not found!");
                    }
                    break;

                case 3: // Search
                    System.out.print("Enter City to Search: ");
                    String searchCity = sc.nextLine();
                    if (cityMap.containsKey(searchCity)) {
                        System.out.println("STD Code of " + searchCity + " = " + cityMap.get(searchCity));
                    } else {
                        System.out.println("City not found!");
                    }
                    break;

                case 4: // Display all
                    System.out.println("\n--- City List ---");
                    for (Map.Entry<String, String> entry : cityMap.entrySet()) {
                        System.out.println(entry.getKey() + " -> " + entry.getValue());
                    }
                    break;

                case 5: // Exit
                    System.out.println("Exiting...");
                    sc.close();
                    return;

                default:
                    System.out.println("Invalid choice! Try again.");
            }
        }
    }
}


6)combobox

import javax.swing.*;
import java.awt.*;
import java.awt.event.*;

public class ComboBoxExampleGUI extends JFrame implements ActionListener {
    JComboBox<String> comboBox;
    JButton showButton;
    JLabel resultLabel;

    public ComboBoxExampleGUI() {
        setTitle("Java Swing Examples");
        setSize(400, 200);
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        setLayout(new FlowLayout());

        JLabel label = new JLabel("Control in action: JComboBox");

        // ComboBox with programming languages
        String languages[] = {"Java", "C", "C++", "Python", "Perl"};
        comboBox = new JComboBox<>(languages);

        showButton = new JButton("Show");
        resultLabel = new JLabel("Language Selected: ");

        add(label);
        add(comboBox);
        add(showButton);
        add(resultLabel);

        showButton.addActionListener(this);

        setVisible(true);
    }

    @Override
    public void actionPerformed(ActionEvent e) {
        String selected = (String) comboBox.getSelectedItem();
        resultLabel.setText("Language Selected: " + selected);
    }

    public static void main(String[] args) {
        new ComboBoxExampleGUI();
    }
}



7)customerfile


import java.io.*;
import java.util.Scanner;

class CustomerFile {
    public static void main(String[] args) throws IOException {
        Scanner sc = new Scanner(System.in);

        FileOutputStream fos = new FileOutputStream("customers.dat");
        DataOutputStream dos = new DataOutputStream(fos);

        System.out.print("Enter number of customers: ");
        int n = sc.nextInt();
        sc.nextLine(); // consume newline

        for (int i = 1; i <= n; i++) {
            System.out.println("\nEnter details of customer " + i + ":");
            System.out.print("Customer ID: ");
            int cid = sc.nextInt();
            sc.nextLine();
            System.out.print("Name: ");
            String name = sc.nextLine();
            System.out.print("Address: ");
            String address = sc.nextLine();
            System.out.print("Mobile No: ");
            String mobile = sc.nextLine();

            dos.writeInt(cid);
            dos.writeUTF(name);
            dos.writeUTF(address);
            dos.writeUTF(mobile);
        }

        dos.close();
        fos.close();

        System.out.println("\nData written successfully to customers.dat");

        FileInputStream fis = new FileInputStream("customers.dat");
        DataInputStream dis = new DataInputStream(fis);

        System.out.println("\n***** Customer Details *****");
        for (int i = 1; i <= n; i++) {
            int cid = dis.readInt();
            String name = dis.readUTF();
            String address = dis.readUTF();
            String mobile = dis.readUTF();

            System.out.println("\nCustomer " + i + ":");
            System.out.println("ID: " + cid);
            System.out.println("Name: " + name);
            System.out.println("Address: " + address);
            System.out.println("Mobile: " + mobile);
        }

        dis.close();
        fis.close();
        sc.close();
    }
}


8)doctorname exception


import java.util.*;

// User-defined Exception
class InvalidNameException extends Exception {
}

class DoctorNameException {
    public static void main(String args[]) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter the Doctor Name ");
        String name = s.next();

        try {
            for (int i = 0; i < name.length(); i++) {
                int ch = (int) name.charAt(i);

                if ((ch >= 65 && ch <= 90) || (ch >= 97 && ch <= 122)) {
                } 
                else {
                    throw new InvalidNameException();
                }
            }
            System.out.println("Doctor Name is : " + name);
        } 
        catch (InvalidNameException e) {
            System.out.println("Invalid Name ");
        }
    }
}



9) employeearray


import java.util.Scanner;

class Employee {
    String name;
    double salary;

    // Constructor
    Employee(String name, double salary) {
        this.name = name;
        this.salary = salary;
    }

    // Method to display employee details
    void displayInfo() {
        System.out.println("Name: " + name + ", Salary: " + salary);
    }
}

public class EmployeeArray1 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Create an array to store 5 Employee objects
        Employee[] employees = new Employee[5];

        // Input employee data
        for (int i = 0; i < employees.length; i++) {
            System.out.println("Enter details for Employee " + (i + 1) + ":");

            System.out.print("Name: ");
            String name = scanner.nextLine();

            System.out.print("Salary: ");
            double salary = scanner.nextDouble();
            scanner.nextLine(); // consume newline

            // Create new Employee object and store in array
            employees[i] = new Employee(name, salary);
 }

        // Display employee data
        System.out.println("\nEmployee Details:");
        for (int i = 0; i < employees.length; i++) {
            System.out.print("Employee " + (i + 1) + ": ");
            employees[i].displayInfo();
        }

        scanner.close();
    }
}


10) empmaxsal

import java.io.*;

class Emp {
    private int id;
    private double salary;
    private String name, dept;
    double total; // total salary (salary + bonus if any)

    public Emp() {
        id = 0;
        salary = 0.0;
        name = "";
        dept = "";
        total = 0.0;
    }

    public Emp(int id, double salary, String name, String dept) {
        this.id = id;
        this.salary = salary;
        this.name = name;
        this.dept = dept;
        this.total = salary;
    }

    public void accept() throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        System.out.print("Enter id of employee: ");
        id = Integer.parseInt(br.readLine());
        System.out.print("Enter name of employee: ");
        name = br.readLine();
        System.out.print("Enter salary of employee: ");
        salary = Double.parseDouble(br.readLine());
        System.out.print("Enter department of employee: ");
        dept = br.readLine();
        total = salary; // initialize total with salary
    }

    public void display() {
        System.out.println("Emp Id: " + id);
        System.out.println("Name: " + name);
        System.out.println("Salary: " + salary);
        System.out.println("Department: " + dept);
    }
}

class Manager extends Emp {
    private double bonus;

    public void accept() throws IOException {
        super.accept(); // take basic emp details
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        System.out.print("Enter Bonus of employee: ");
        bonus = Double.parseDouble(br.readLine());
        super.total = super.total + bonus; // add bonus to total salary
    }

    public void display() {
        super.display();
        System.out.println("Bonus: " + bonus);
        System.out.println("Total Salary: " + total);
    }
}

public class EmpMaxSalary {
    public static void main(String[] args) throws IOException {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

        System.out.print("Enter the number of employees: ");
        int n = Integer.parseInt(br.readLine());

        Manager[] m = new Manager[n];

        // Accept employees
        for (int i = 0; i < n; i++) {
            m[i] = new Manager();
            m[i].accept();
        }

        // Display employees
        for (int i = 0; i < n; i++) {
            m[i].display();
            System.out.println("---------------------------------");
        }

        // Find max salary employee
        double max = m[0].total;
        int index = 0;
        for (int i = 1; i < n; i++) {
            if (m[i].total > max) {
                max = m[i].total;
                index = i;
            }
        }

        System.out.println("The Employee having the maximum Total salary is:");
        m[index].display();
    }
}


11) Fibonacci

import java.util.Scanner;

public class FibonacciSeries {

    // Function to print Fibonacci series
    static void printFibonacci(int n) {
        int a = 0, b = 1, c;
        System.out.print("Fibonacci Series: " + a + " " + b);

        for (int i = 2; i < n; i++) {
            c = a + b;
            System.out.print(" " + c);
            a = b;
            b = c;
        }
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number of terms: ");
        int n = sc.nextInt();

        printFibonacci(n);
        sc.close();
    }
}


12) fileinfo

import java.io.File;
import java.util.Scanner;

public class FileInfo {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // Accept file name from user
        System.out.print("Enter file name with path: ");
        String fileName = sc.nextLine();

        File f = new File(fileName);

        if (f.exists()) {
            System.out.println("\nFile Information:");
            System.out.println("File Name: " + f.getName());
            System.out.println("Absolute Path: " + f.getAbsolutePath());
            System.out.println("Readable: " + f.canRead());
            System.out.println("Writable: " + f.canWrite());
            
            // Checking type (file or directory)
            if (f.isFile()) {
                System.out.println("Type: Regular File");
            } else if (f.isDirectory()) {
                System.out.println("Type: Directory");
            }
            
            System.out.println("File Size: " + f.length() + " bytes");
        } else {
            System.out.println("\nFile does not exist!");
        }

        sc.close();
    }
}


14) hashtable

import java.io.*;
import java.util.*;

class Hashtable1 {
    public static void main(String args[]) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        Hashtable ht = new Hashtable();
        float sal;
        String name = null;

        System.out.println("\nEnter number of Employees : ");
        int n = Integer.parseInt(br.readLine());

        // Input employee details
        for (int i = 1; i <= n; i++) {
            System.out.print("Enter Employee name : ");
            name = br.readLine();

            System.out.print("Enter Employee Salary : ");
            sal = Float.parseFloat(br.readLine());

            ht.put(name, sal);   // store name & salary in Hashtable
        }

        // Display complete Hashtable
        System.out.println("Hash table = " + ht);

        // Display using Enumeration
        Enumeration nameEnum = ht.keys();
        Enumeration salaryEnum = ht.elements();

        System.out.println("Name\tSalary");

        while (nameEnum.hasMoreElements()) {
            System.out.println(nameEnum.nextElement() + "\t" + salaryEnum.nextElement());
        }

        // Search employee
        System.out.println("Enter name to be searched : ");
        String snm = br.readLine();

        nameEnum = ht.keys();
        salaryEnum = ht.elements();
        int cnt = 0;

        while (nameEnum.hasMoreElements()) {
            name = (String) nameEnum.nextElement();
            if (snm.equals(name)) {
                cnt++;
                System.out.println("Salary = " + ht.get(name));
            }
        }

        if (cnt == 0) {
            System.out.println("Employee not found");
        }
    }
}



15) interfaceshape


import java.io.*;

// Interface
interface Shape {
    void calc_area();
    void calc_volume();
    float pi = 3.14f;   // final and static by default in interface
}

class Sphere implements Shape {
    double r;
    private double area;
    private double volume;

    public void accept() throws IOException {
        System.out.println("Enter the radius of the Sphere: ");
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        r = Double.parseDouble(br.readLine());
    }

    public void calc_area() {
        area = 4 * pi * r * r;
    }

    public void calc_volume() {
        volume = (4.0 / 3.0) * pi * r * r * r;
    }

    public void display() {
        calc_area();
        calc_volume();
        System.out.println("The area of Sphere is: " + area);
        System.out.println("The volume of Sphere is: " + volume);
    }
}

class Cone implements Shape {
    double h, r, area, volume;

    public void accept() throws IOException {
        System.out.println("Enter radius and height of the Cone: ");
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        r = Double.parseDouble(br.readLine());
        h = Double.parseDouble(br.readLine());
    }

    public void calc_area() {
        double sq = h * h + r * r;
        area = pi * r * (r + Math.sqrt(sq));
    }

    public void calc_volume() {
        volume = (pi * r * r * h) / 3;
    }

    public void display() {
        calc_area();
        calc_volume();
        System.out.println("The area of Cone is: " + area);
        System.out.println("The volume of Cone is: " + volume);
    }
}

class Cylinder implements Shape {
    double r, h, area, volume;

    public void accept() throws IOException {
        System.out.println("Enter radius and height of the Cylinder: ");
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        r = Double.parseDouble(br.readLine());
        h = Double.parseDouble(br.readLine());
    }

    public void calc_area() {
        area = (2 * pi * r * h) + (2 * pi * r * r);
    }

    public void calc_volume() {
        volume = pi * r * r * h;
    }

    public void display() {
        calc_area();
        calc_volume();
        System.out.println("The area of Cylinder is: " + area);
        System.out.println("The volume of Cylinder is: " + volume);
    }
}

class Box implements Shape {
    double l, b, h, area, volume;

    public void accept() throws IOException {
        System.out.println("Enter length, breadth and height of the Box: ");
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        l = Double.parseDouble(br.readLine());
        b = Double.parseDouble(br.readLine());
        h = Double.parseDouble(br.readLine());
    }

    public void calc_area() {
        area = (2 * l * b) + (2 * b * h) + (2 * l * h);
    }

    public void calc_volume() {
        volume = l * b * h;
    }

    public void display() {
        calc_area();
        calc_volume();
        System.out.println("The area of Box is: " + area);
        System.out.println("The volume of Box is: " + volume);
    }
}

public class InterfaceShape {
    public static void main(String[] args) throws IOException {
        Sphere s = new Sphere();
        s.accept();
        s.display();

        Cone co = new Cone();
        co.accept();
        co.display();

        Cylinder cy = new Cylinder();
        cy.accept();
        cy.display();

        Box b = new Box();
        b.accept();
        b.display();
    }
}

16) linklist

import java.util.*;

class Linklist {
    public static void main(String[] args) {
        LinkedList ll = new LinkedList();

        // Add elements
        ll.add("Red");
        ll.add("Blue");
        ll.add("Yellow");
        ll.add("Orange");

        // Display elements using Iterator
        Iterator i = ll.iterator();
        System.out.println("\nContents of the List using an Iterator:\n");
        while (i.hasNext()) {
            String s = (String) i.next();
            System.out.println(s);
        }

        // ListIterator to traverse in both directions
        ListIterator<String> li = ll.listIterator();

        // Move the iterator to the end of the list
        while (li.hasNext()) {
            li.next();
        }

        // Display elements in reverse order
        System.out.println("\nContents of the List in reverse order using a ListIterator:");
        while (li.hasPrevious()) {
            System.out.println(li.previous());
        }

        // Add elements at specific positions
        ll.add(2, "Pink");   // add at index 2 (3rd position)
        ll.add(3, "Green");  // add at index 3 (4th position)

        // Display modified list
        System.out.println("\nList after inserting elements between Blue and Yellow:");
        System.out.println(ll);
    }
}


17) loginpage

import javax.swing.*;
import java.awt.*;
import java.awt.event.*;

public class LoginPageGUI extends JFrame implements ActionListener {
    JLabel userLabel, passLabel;
    JTextField userText;
    JPasswordField passText;
    JButton loginBtn, cancelBtn;

    public LoginPageGUI() {
        setTitle("Login Page");
        setSize(350, 200);
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        setLocationRelativeTo(null); // Center the window

        // Layout
        setLayout(new GridLayout(3, 2, 10, 10));

        // Components
        userLabel = new JLabel("Username:");
        passLabel = new JLabel("Password:");
        userText = new JTextField();
        passText = new JPasswordField();
        loginBtn = new JButton("Login");
        cancelBtn = new JButton("Cancel");

        // Add components to layout
        add(userLabel);
        add(userText);
        add(passLabel);
        add(passText);
        add(loginBtn);
        add(cancelBtn);

        // Add ActionListeners
        loginBtn.addActionListener(this);
        cancelBtn.addActionListener(this);

        setVisible(true);
    }

    public void actionPerformed(ActionEvent e) {
        if (e.getSource() == loginBtn) {
            String user = userText.getText();
            String pass = new String(passText.getPassword());

            if (user.equals("admin") && pass.equals("1234")) {
                JOptionPane.showMessageDialog(this, "Login Successful!");
            } else {
                JOptionPane.showMessageDialog(this, "Invalid Username or Password!");
            }
        } else if (e.getSource() == cancelBtn) {
            userText.setText("");
            passText.setText("");
        }
    }

    public static void main(String[] args) {
        new LoginPageGUI();
    }
}

